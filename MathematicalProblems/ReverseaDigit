

https://www.geeksforgeeks.org/problems/reverse-digit0316/1


class Solution {
    public int reverseDigits(int n) {
        // Code here
         int temp=n;
        int sum=0;
        int r=0;
        while(temp>0){
            r=temp%10;
            sum=sum*10+r;
            temp=temp/10;
            
        }
        
       return sum;
    }
}
