
https://www.geeksforgeeks.org/problems/special-series-sum0903/1


//User function Template for Java
class Solution {
    static long sumOfTheSeries(long n){
        // code here
        long sum=0;
        long specialseriessum=0;
        for(int i=1;i<=n;i++){
            sum=sum+i;
            specialseriessum=sum+specialseriessum;
            
        }
        return specialseriessum;
    }
}
