

https://www.geeksforgeeks.org/problems/palindrome0746/1


// User function Template for Java

class Solution {
    public boolean isPalindrome(int n) {
        // Code here
        int temp=n;
        int sum=0;
        int r=0;
        while(temp>0){
            r=temp%10;
            sum=sum*10+r;
            temp=temp/10;
            
        }
        if(sum==n){
            return true;
        }
        else{
           return false;
        }
    }
}
